<h1> Dinosaurs </h1>
 [ <a href="dinosaurs">Dinosaurs</a> | <a href="species">Species</a> | <a href="employees">Employees</a> | <a href="visitors">Visitors</a> | <a href="dinosaurAssignments">Dinosaur Assignments</a> | <a href="locations">Locations</a> ]

<table>
    <thead>
    <tr>
        <!-- Iterate through each key in the first entry to get the
        column name -->
        {% for key in Dinosaurs[0].keys() %}

        <!-- Create a <th> tag with the key inside of it, this will be
        our header row -->
        <th>{{ key }}</th>

        <!-- End of this for loop -->
        {% endfor %}
    </tr>
    <tbody>

    <!-- Now, iterate through every person in Dinosaurs -->
    {% for dinosaur in Dinosaurs %}
    <tr>
        <!-- Then iterate through every key in the current person dictionary -->
        {% for key in dinosaur.keys() %}

        <!-- Create a <td> element with the value of that key in it -->
        <td>{{dinosaur[key]}}</td>
        {% endfor %}
    </tr>
    {% endfor %}
    </table>